{
  "name": "assetsally",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean",
    "db:migrate": "pnpm --filter @assetsally/database migrate:dev",
    "db:seed": "pnpm --filter @assetsally/database seed",
    "db:studio": "pnpm --filter @assetsally/database studio",
    "db:generate": "pnpm --filter @assetsally/database generate"
  },
  "devDependencies": {
    "@prisma/client": "^5.10.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-react": "^5.1.4",
    "bcrypt": "^5.1.1",
    "cross-env": "^10.1.0",
    "happy-dom": "^20.6.1",
    "jsdom": "^28.1.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}